#!/usr/bin/env bash
# This script displays numbers from 1 to 100 based on FizzBuzz rules

# Loop through numbers from 1 to 100
for ((i=1; i<=100; i++)); do
    # Check FizzBuzz conditions
    if ((i % 3 == 0)) && ((i % 5 == 0)); then
        echo "FizzBuzz"
    elif ((i % 3 == 0)); then
        echo "Fizz"
    elif ((i % 5 == 0)); then
        echo "Buzz"
    else
        echo "$i"
    fi
done
